<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="composeTitle" autocomplete="off">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea rows="5" type="text" class="form-control " name="composeContent" autocomplete="off"></textarea>
  </div>
  <div class="form-group">
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </div>
</form>


<%- include("partials/footer"); -%>
